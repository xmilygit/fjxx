<!-- Modal -->
<div class="modal" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">图片素材</h4>
      </div>
      <div class="modal-body">
        {{#each imgs.item}}
        <img src="http://read.html5.qq.com/image?src=forum&q=5&r=0&imgflag=7&imageUrl={{wechatimgurl url}}" class="img-thumbnail" width="150">
        <img src="http://read.html5.qq.com/image?src=forum&q=5&r=0&imgflag=7&imageUrl={{wechatimgurl url}}" class="img-thumbnail" width="150">
        <img src="http://read.html5.qq.com/image?src=forum&q=5&r=0&imgflag=7&imageUrl={{wechatimgurl url}}" class="img-thumbnail" width="150">
        <img src="http://read.html5.qq.com/image?src=forum&q=5&r=0&imgflag=7&imageUrl={{wechatimgurl url}}" class="img-thumbnail" width="150">
        <img src="http://read.html5.qq.com/image?src=forum&q=5&r=0&imgflag=7&imageUrl={{wechatimgurl url}}" class="img-thumbnail" width="150">
        <img src="http://read.html5.qq.com/image?src=forum&q=5&r=0&imgflag=7&imageUrl={{wechatimgurl url}}" class="img-thumbnail" width="150">
        {{/each}}
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
        <button type="button" class="btn btn-primary">插入</button>
      </div>
    </div>
  </div>
</div>
<!--/Modal-->

<div class="alert alert-info">
    <p style="font-size:18px">{{title}}</p>
</div>
<div class="well well-sm" id="list1">
    <span class="badge">1</span>
    <!--<span class="badge">2</span>
    <span class="badge">3</span>
    <span class="badge">4</span>
    <span class="badge">5</span>
    <span class="badge">6</span>
    <span class="badge">7</span>
    <span class="badge">8</span>-->
    <div class="pull-right">
        <button class="btn btn-success btn-xs" type="submit">删除</button>
        <button class="btn btn-success btn-xs" type="submit">添加</button>
    </div>
</div>
<form>
    <div class="form-group">
        <img  data-toggle="modal" data-target="#myModal" src="data:image/svg+xml;base64," alt="..." class="img-thumbnail">
    </div>
    <div class="form-group">
        <input type="text" class="form-control" id="ipt-title" name="pttitle" placeholder="标题">
    </div>
    <div class="form-group">
        <input type="text" class="form-control" id="ipt-digest" name="ptdigest" placeholder="摘要">
    </div>
    <div class="form-group">
        <input type="text" class="form-control" id="ipt-author" name="ptauthor" placeholder="作者">
    </div>
    <div class="form-group">
        <textarea rows='7' class="form-control" id="ipt-content" name="content" placeholder="主要内容"></textarea>
    </div>





    <div class="form-group">
        <p><input type="checkbox" value="" id="my-checkbox1" name="my-checkbox1">
            <input type="checkbox" id="my-checkbox2" name="show_cover_pic" checked></p>
        <input type="text" class="form-control" id="ipt-content-clickurl" name="content-clickurl" placeholder="点击后跳转链接">

    </div>

    <p><button type="submit" class="btn btn-default btn-block">提交</button></p>
</form>
<br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br>
<br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br>
<script src="/javascripts/stickUp.min.js"></script>
<script type="text/javascript">
    jQuery(function ($) {
        $(document).ready(function () {
            $("#list1").stickUp({topMargin: 'auto'});
        });
    });
    $(function () {

        $("#ipt-content-clickurl").hide();
        $("#my-checkbox1").bootstrapSwitch({
            size: 'mini',
            onText: '点击后跳转',
            offText: '点击后不跳转',
            offColor: 'warning',
            onSwitchChange: function (event, state) {
                if (state) {
                    $("#ipt-content-clickurl").show();
                } else {
                    $("#ipt-content-clickurl").hide();
                }
            }
        });

        $("#my-checkbox2").bootstrapSwitch({
            size: 'mini',
            onText: '显示封面',
            offText: '不显示封面',
            offColor: 'warning'
        });
    })

</script>