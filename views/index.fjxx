{{#if error}}
<div class="am-modal am-modal-alert" tabindex="-1" id="error-message">
  <div class="am-modal-dialog">
    <div class="am-modal-hd">登录出现错误</div>
    <div class="am-modal-bd">
      {{#each error}} {{this}} {{/each}}
    </div>
    <div class="am-modal-footer">
      <span class="am-modal-btn">确定</span>
    </div>
  </div>
</div>
<script>
$(function() {
    $('#error-message').modal('open');
});
</script>
{{/if}}
{{#if user}} {{user.username}} {{/if}}